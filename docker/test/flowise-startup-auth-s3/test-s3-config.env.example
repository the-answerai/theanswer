# Example S3 Test Configuration
# Copy this file to test-s3-config.env and replace with actual values

# Storage configuration (valid types: local, s3, gcs)
STORAGE_TYPE=s3
S3_STORAGE_BUCKET_NAME=your-test-bucket-name
DEBUG=true
VERBOSE=true
LOG_LEVEL=debug

# Database configuration for testing
DATABASE_TYPE=postgres
DATABASE_HOST=postgres
DATABASE_PORT=5432
DATABASE_NAME=example_db
DATABASE_USER=example_user
DATABASE_PASSWORD=example_password

# Redis configuration for testing
REDIS_URL=redis://redis:6379
AAI_DEFAULT_REDIS_URL=redis://redis:6379

# Application configuration
PORT=4000
NODE_ENV=production
DISABLE_FLOWISE_TELEMETRY=true
APIKEY_STORAGE_TYPE=db

# Auth0 configuration for testing (replace with actual values)
AUTH0_JWKS_URI=https://your-domain.auth0.com/.well-known/jwks.json
AUTH0_ISSUER_BASE_URL=https://your-domain.auth0.com
AUTH0_BASE_URL=https://your-app-domain.com
AUTH0_AUDIENCE=https://your-audience.com
AUTH0_SCOPE="openid profile email"
AUTH0_TOKEN_SIGN_ALG=RS256
AUTH0_DOMAIN="your-domain.auth0.com"
AUTH0_ORGANIZATION_ID=org_your_org_id
AUTH0_CLIENT_ID=your_client_id
AUTH0_CLIENT_SECRET=your_client_secret
# AUTH0_SECRET is not needed for RS256 JWT validation (uses JWKS instead)
